meta {
  name: Get A Specific Model
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/v1/models/:model_id
  body: none
  auth: inherit
}

params:path {
  model_id: 123
}

headers {
  anthropic-version: 2023-06-01
  x-api-key: test-key-abc-123
  Content-Type: application/json
  anthropic-beta: BETA_FEATURE_NAME
  anthropic-dangerous-direct-browser-access: true
}

example {
  name: Claude Sonnet 4 model info
  
  request: {
    url: {{baseUrl}}/v1/models/:model_id
    method: GET
    mode: none
    params:path: {
      model_id: 123
    }
  
    headers: {
      anthropic-version: 2023-06-01
      ~anthropic-beta: BETA_FEATURE_NAME
      x-api-key: test-key-abc-123
      Content-Type: application/json
      ~anthropic-dangerous-direct-browser-access: true
    }
  }
  
  response: {
    headers: {
      Content-Type: application/json
    }
  
    status: {
      code: 200
      text: OK
    }
  
    body: {
      type: json
      content: '''
        {
          "id": "claude-sonnet-4-20250514",
          "type": "model",
          "display_name": "Claude Sonnet 4",
          "created_at": "2025-05-14T00:00:00Z"
        }
      '''
    }
  }
}

